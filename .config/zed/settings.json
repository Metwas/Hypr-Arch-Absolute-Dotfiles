{
       "ui_font_weight": 500.0,
       "icon_theme": "Catppuccin Latte",
       "disable_ai": true,
       "buffer_line_height": {
              "custom": 1.99,
       },
       "edit_predictions": {
              "enabled_in_text_threads": false,
       },
       "languages": {
              "TypeScript": {
                     "format_on_save": "on",
                     "formatter": "prettier",
              },
              "Rust": {
                     "format_on_save": "on",
                     "formatter": "prettier",
              },
              "C++": {
                     "format_on_save": "on",
                     "formatter": "prettier",
              },
       },
       "show_edit_predictions": false,
       "agent": {
              "expand_terminal_card": false,
              "expand_edit_card": false,
              "enable_feedback": false,
              "single_file_review": false,
              "model_parameters": [],
       },
       "zoomed_padding": true,
       "use_system_window_tabs": false,
       "bottom_dock_layout": "contained",
       "preview_tabs": {
              "enabled": true,
       },
       "tab_bar": {
              "show_nav_history_buttons": false,
              "show": false,
       },
       "title_bar": {
              "show_menus": false,
              "show_branch_icon": true,
              "show_project_items": true,
              "show_branch_name": true,
              "show_sign_in": false,
              "show_user_picture": false,
              "show_onboarding_banner": false,
       },
       "project_panel": {
              "button": false,
              "hide_root": true,
              "scrollbar": {
                     "show": "never",
              },
       },
       "file_finder": {
              "modal_max_width": "xlarge",
       },
       "buffer_font_weight": 900.0,
       "buffer_font_family": ".ZedMono",
       "telemetry": {
              "diagnostics": false,
              "metrics": false,
       },
       "vim_mode": false,
       "base_keymap": "VSCode",
       "status_bar": {
              "active_language_button": false,
              "experimental.show": false,
       },
       "minimap": {
              "thumb_border": "left_open",
              "thumb": "always",
              "show": "auto",
       },
       "hover_popover_delay": 300,
       "hover_popover_enabled": true,
       "current_line_highlight": "all",
       "cursor_shape": "bar",
       "cursor_blink": true,
       "show_whitespaces": "trailing",
       "tab_size": 7,
       "ui_font_size": 14,
       "buffer_font_size": 11.0,
       "theme": "Dark Knight",
       "profiles": {
              "light": {
                     "theme": "One Light",
              },
       },
}
